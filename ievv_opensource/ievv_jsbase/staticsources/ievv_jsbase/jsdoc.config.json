{
  "markdown": {
    "parser": "gfm"
  },
  "opts": {
    "destination": "./built_docs/",
    "encoding": "utf8",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [
      "./scripts/javascript/"
    ],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}